{
    "configurations": [
        {
            
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "cStandard": "gnu99",
            "cppStandard": "c++17",
            "name": "stm32f411ce",
            "compilerPath": "${ARMGCC_DIR}/arm-none-eabi-gcc",
            "intelliSenseMode": "linux-gcc-arm",
            "compilerArgs": [
                            "-mcpu=cortex-m4",
                            "-mthumb",
                            "-mfpu=fpv4-sp-d16",
                            "-mfloat-abi=hard"
                        ],
            "defines": [
                            "USE_HAL_DRIVER",
                            "STM32F411xE"
                        ]
        }
    ],
    "version": 4
}